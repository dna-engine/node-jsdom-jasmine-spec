{
   "name": "node-jsdom-jasmine-spec",
   "description": "Sample dna-engine project running on Node.js and jsdom executing Jasmine test cases",
   "version": "1.0.0",
   "license": "MIT",
   "type": "module",
   "module": "app.js",
   "main": "app.js",
   "repository": {
      "type": "git",
      "url": "git+https://github.com/dna-engine/node-jsdom-jasmine-spec.git"
   },
   "homepage": "https://github.com/dna-engine/node-jsdom-jasmine-spec",
   "bugs": "https://github.com/dna-engine/node-jsdom-jasmine-spec/issues",
   "docs": "https://dna-engine.org/docs",
   "author": "Center Key (https://centerkey.com)",
   "keywords": [
      "dna-engine",
      "node",
      "jsdom",
      "jasmine",
      "javascript"
   ],
   "jshintConfig": {
      "esversion": 11,
      "strict": "implied",
      "eqeqeq": true,
      "undef": true,
      "unused": true,
      "node": true,
      "jasmine": true
   },
   "scripts": {
      "pretest": "jshint . --exclude-path .gitignore",
      "test": "jasmine --reporter=jasmine-ts-console-reporter"
   },
   "dependencies": {
      "dna-engine": "~3.0"
   },
   "devDependencies": {
      "assert-deep-strict-equal": "~1.1",
      "canvas": "~2.11",
      "jasmine": "~5.1",
      "jasmine-ts-console-reporter": "~3.1",
      "jsdom": "~22.1",
      "jshint": "~2.13"
   }
}
